{
  "build": {
    "content": [
      {
        "files": [
          "articles/**/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/Images/**",
          "images/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template":[
      "default",
      "templates/enova"],
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_enableSearch": "true",
      "_gitContribute": {
        "repo": "https://github.com/soneta/docfx-soneta",
        "branch": "dev"
      },
      "_gitUrlPattern": "github"
  }
}
}